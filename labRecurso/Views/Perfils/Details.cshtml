@model labRecurso.Models.Perfil

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes</h1>

<div>
    <h4>Perfil</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.utilizador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.utilizador.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.saldo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.saldo)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Voltar</a>
</div>
